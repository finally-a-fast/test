{
    "name": "finally-a-fast/fafcms-test",
    "keywords": [
        "yii2",
        "fafcms",
        "fafcms-helpers"
    ],
    "type": "library",
    "description": "This package provides basic helpers for the Finally a fast CMS.",
    "license": "MIT",
    "homepage": "https://www.finally-a-fast.com/",
    "support": {
        "issues": "https://gitlab.com/finally-a-fast/fafcms-helpers/issues",
        "source": "https://gitlab.com/finally-a-fast/fafcms-helpers",
        "docs": "https://www.finally-a-fast.com/packages/fafcms-helpers/docs"
    },
    "authors": [
        {
            "name": "Christoph MÃ¶ke",
            "email": "christophmoeke@gmail.com",
            "role": "Project Leader"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "finally-a-fast/fafcms-core": "dev-master",
        "robinherbots/jquery.inputmask": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "bootstrap": "fafcms\\cookieconsent\\Bootstrap",
        "fafcms": {
            "name": "Helpers",
            "logo": "https://www.finally-a-fast.com/packages/fafcms-helpers/assets/logo.jpg",
            "header": "https://www.finally-a-fast.com/packages/fafcms-helpers/assets/header.jpg",
            "docs": {
                "description": "https://www.finally-a-fast.com/packages/fafcms-helpers/docs/description.md",
                "support": "https://www.finally-a-fast.com/packages/fafcms-helpers/docs/support.md",
                "changelog": "https://www.finally-a-fast.com/packages/fafcms-helpers/docs/changelog.md",
                "developers": "https://www.finally-a-fast.com/packages/fafcms-helpers/docs/developers.md"
            }
        }
    },
    "scripts": {
        "build": [
            "echo hello"
        ]
    },
    "autoload": {
        "psr-4": {
            "fafcms\\helpers\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
